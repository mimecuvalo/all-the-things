{
  "name": "all-the-things",
  "private": true,
  "description": "all the bells ðŸ”” and whistles ðŸ˜™ for next.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mimecuvalo/all-the-things.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.17.0"
  },
  "bugs": {
    "url": "https://github.com/mimecuvalo/all-the-things/issues"
  },
  "scripts": {
    "predev": "yarn i18n:compile",
    "dev": "open http://localhost:3000 && next dev",
    "prebuild": "yarn i18n:compile",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "i18n:extract": "formatjs extract 'pages/**/*.ts*' 'components/**/*.ts*' --out-file i18n/en.json --additional-component-names F --id-interpolation-pattern '[md5:contenthash:hex:10]' --format smartling",
    "i18n:compile": "formatjs compile-folder i18n i18n-compiled-lang --format smartling",
    "alex": "alex .",
    "analyze": "source-map-explorer .next/static/**/*.js",
    "codegen": "graphql-codegen",
    "config": "node bin/config.js",
    "prepare": "husky install",
    "prettier:all": "prettier --check .",
    "prettier:fix": "prettier --write",
    "prettier:fix:all": "prettier --write .",
    "postinstall": "prisma generate",
    "ts-script": "ts-node-script -r tsconfig-paths/register",
    "build-storybook": "build-storybook -s public",
    "storybook": "storybook dev -p 9001 -c .storybook --ci --quiet",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "3.8.7",
    "@auth0/nextjs-auth0": "3.2.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.16",
    "@mui/material": "5.14.16",
    "@prisma/client": "5.5.2",
    "@sentry/nextjs": "7.77.0",
    "@vercel/analytics": "1.1.1",
    "apollo-server-micro": "3.10.3",
    "babel-plugin-formatjs": "10.5.7",
    "body-parser": "1.20.2",
    "circular-dependency-plugin": "5.2.2",
    "classnames": "2.3.2",
    "cookie-parser": "1.4.6",
    "dataloader": "2.2.2",
    "deepmerge": "4.3.1",
    "graphql": "16.8.1",
    "graphql-resolvers": "0.4.2",
    "graphql-tools": "8.3.7",
    "js-cookie": "3.0.5",
    "lodash": "4.17.21",
    "md5.js": "1.3.5",
    "micro": "10.0.1",
    "next": "14.0.1",
    "prisma": "5.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.5.1",
    "react-spring": "9.7.3",
    "sanitize.css": "13.0.0",
    "sharp": "0.32.6",
    "uuid": "9.0.1",
    "web-vitals": "3.5.0",
    "workbox-background-sync": "7.0.0",
    "workbox-broadcast-update": "7.0.0",
    "workbox-cacheable-response": "7.0.0",
    "workbox-core": "7.0.0",
    "workbox-expiration": "7.0.0",
    "workbox-google-analytics": "7.0.0",
    "workbox-navigation-preload": "7.0.0",
    "workbox-precaching": "7.0.0",
    "workbox-range-requests": "7.0.0",
    "workbox-routing": "7.0.0",
    "workbox-strategies": "7.0.0",
    "workbox-streams": "7.0.0"
  },
  "devDependencies": {
    "@formatjs/cli": "6.2.1",
    "@graphql-codegen/add": "5.0.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@storybook/addon-essentials": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/nextjs": "7.5.2",
    "@storybook/react": "7.5.2",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/graphql-resolvers": "0.3.2",
    "@types/jest": "29.5.7",
    "@types/js-cookie": "3.0.5",
    "@types/lodash": "4.14.200",
    "@types/node": "20.8.10",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "@types/uuid": "9.0.6",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "alex": "11.0.1",
    "axe-core": "4.8.2",
    "babel-jest": "29.7.0",
    "cross-fetch": "4.0.0",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.1",
    "eslint-plugin-formatjs": "4.11.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-testing-library": "6.1.0",
    "husky": "8.0.3",
    "import-sort-style-eslint": "6.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "prettier-plugin-import-sort": "0.0.7",
    "source-map-explorer": "2.5.3",
    "storybook": "7.5.2",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "**/*/(schema|resolvers)/*.ts": [
      "yarn codegen"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "eslint"
    }
  },
  "husky": {
    "hooks": {
      "post-checkout": "./bin/post-checkout",
      "post-merge": "./bin/post-merge"
    }
  },
  "browserslist": "last 2 versions",
  "sideEffects": false
}
